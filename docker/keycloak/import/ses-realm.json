{
  "realm": "ses-manager",
  "displayName": "SES Manager",
  "enabled": true,
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "accessTokenLifespan": 300,
  "ssoSessionIdleTimeout": 1800,
  "ssoSessionMaxLifespan": 36000,
  "roles": {
    "realm": [
      {
        "name": "admin",
        "description": "Administrator role"
      },
      {
        "name": "project_manager",
        "description": "Project Manager role"
      },
      {
        "name": "engineer",
        "description": "Engineer role"
      },
      {
        "name": "client",
        "description": "Client role"
      },
      {
        "name": "accountant",
        "description": "Accountant role"
      }
    ]
  },
  "clients": [
    {
      "clientId": "ses-backend",
      "name": "SES Backend Service",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "ses-backend-secret",
      "redirectUris": ["http://localhost:*"],
      "webOrigins": ["*"],
      "protocol": "openid-connect",
      "attributes": {
        "access.token.lifespan": "300"
      },
      "defaultClientScopes": ["web-origins", "role_list", "profile", "roles", "email"],
      "optionalClientScopes": ["address", "phone", "offline_access", "microprofile-jwt"]
    },
    {
      "clientId": "ses-frontend",
      "name": "SES Frontend Application",
      "enabled": true,
      "publicClient": true,
      "redirectUris": ["http://localhost:3000/*", "http://localhost:8081/*"],
      "webOrigins": ["*"],
      "protocol": "openid-connect",
      "defaultClientScopes": ["web-origins", "role_list", "profile", "roles", "email"],
      "optionalClientScopes": ["address", "phone", "offline_access"]
    }
  ],
  "users": [
    {
      "username": "admin",
      "enabled": true,
      "emailVerified": true,
      "firstName": "System",
      "lastName": "Administrator",
